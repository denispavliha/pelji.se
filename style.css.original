@import url('https://fonts.googleapis.com/css2?family=Roboto&display=swap');

html, body
{
	margin: 0;
	padding: 0;

	background: #EEF3EE; 
	font-family: 'Roboto', sans-serif;

	-webkit-user-select: none; 
	-ms-user-select: none; 
	user-select: none; 

	cursor: default;
}  

.viewport
{
	position: absolute;
	top: 0;
	left: 0;
	right: 0;

	background: #EEF3EE; 
	display: none; 
}

.viewport > .container
{
	padding-bottom: 140px;
}

#map
{
	display: block;
}

#map > p
{
	display: none;
	
	width: 100%;
	padding: 40px 20px;

	text-align: center; 

	font-family: 'Roboto', sans-serif;
	font-size: 12px;
}

#map > p > a
{
	white-space: nowrap;
}

.menu-container
{
	display: block; 

	position: fixed;
	bottom: 50px;
	padding: 0;

	width: 100%; 

	z-index: 5000 !important; 
}

.menu
{
	position: relative;
	display: block;
	margin-left: auto;
	margin-right: auto;

	width: fit-content;

	box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.20);
	-webkit-box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.20);
	-moz-box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.20);
	
	border-radius: 5px;
	border: 1px solid #d0d0d0;
	background: #ffffff;
}

.menu-item
{
	display: inline-block;
	min-width: 80px;
	padding: 5px;

	text-align: center;
	text-decoration: none;

	color: #A8ACA8;	
}

.menu-item:hover, .menu-item-active
{
	color: #BA1200;
}

.menu-item > i
{
	font-size: 1.6rem;
}

.menu-item > div
{
	display: block;
	text-transform: uppercase;
	font-size: 0.8rem
}

.menu-item-tracking
{
	display: none;
}

.menu-item-tracking, .menu-item-tracking:hover, .btn-danger, .btn-danger:active 
{
	color: #FFFFFF !important;
	background: #BA1200 !important;
}

img.about-logo
{
	display: inline;
	position: relative; 
	bottom: 4px; 
	margin-right: 5px;
}

.about-box
{
	background: #feffff;
	border-radius: 5px;

	padding: 15px 30px;
	margin: 15px;

	height: 100%;
}

