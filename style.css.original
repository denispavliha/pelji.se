.btn-tooltip-tracking-off
{
	display: none;
	background: #BA1200 !important;
	color: #ffffff !important;
	margin-left: 10px;
}

.btn-bus:not(.btn-tooltip-tracking-off)
{
	background: #F0F1F4 !important;
	border: 0;

	-webkit-transition: box-shadow 0.15s ease-in-out 0s;
    -moz-transition: box-shadow 0.15s ease-in-out 0s;
    -o-transition: box-shadow 0.15s ease-in-out 0s;
    -ms-transition: box-shadow 0.15s ease-in-out 0s;
    transition: box-shadow 0.15s ease-in-out 0s;
}

.btn-bus:hover
{
	background: #F4F5F8 !important;

	box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.20);
	-webkit-box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.20);
	-moz-box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.20);
}

.btn
{
	padding-top: 4px;
	padding-bottom: 5px;
}

#btn-tracking-off
{
	display: none;
}

div.track-ico
{
	display: inline-block;

	width: 16px;
	height: 16px;

	background-image: url('images/tracker.png');

	position: relative;
	top: 4px;

	margin-right: 10px;
}

#notification
{
	display: none; 	

    position: fixed;
    top: 0;
    left: 0;
    right: 0;
    width: 100%; 
    z-index: 9000;

	padding: 10px 0;
	text-align: center;
	font-size: 14px;

	border-bottom: 1px solid #808080;
	background: #BA1200;
	color: #feffff;

	box-shadow: inset 0px 0px 20px 0px rgba(0,0,0,0.1);
	-webkit-box-shadow: inset 0px 0px 20px 0px rgba(0,0,0,0.1);
	-moz-box-shadow: inset 0px 0px 20px 0px rgba(0,0,0,0.1);

} 

h1
{
	color: #BA1200;
}

.viewport > h1
{
	background: #BA1200;
	color: #ffffff;
	padding: 15px 0;

	position: fixed;
	top: 0;
	left: 0;
	right: 0; 

	z-index: 2;
}

.accent
{
	color: #BA1200;
}

.libraries-wrapper
{
	color: #A8ACA8; 
	font-size: 12px;
	text-align: center;

	max-width: 650px;

	display: block;
	margin-left: auto;
	margin-right: auto;
} 

.libraries > .library
{
	display: inline;
	white-space: nowrap;
}

.libraries > .library:hover
{
	text-decoration: underline !important;
}

.libraries > .library::after
{
	content: " | ";
}

.libraries a, .libraries a:active, .libraries a:visited, .libraries a:hover
{
	color: rgba(0,0,0,0.4);
	text-decoration: none;
}

h2.route
{
	padding-top: 30px;
	font-size: 2rem;
}

h2.route::before
{
	content: 'Proga Å¡t. ';
} 

h3
{
	padding-top: 20px;
	font-size: 1rem;
} 

span.route-hour
{
	display: inline-block;
	margin-right: 5px;
	margin-bottom: 5px;

	padding: 2px 10px;

	border-radius: 5px;
	border: 1px solid #A8ACA8;
	color: #A8ACA8;
}

span.route-hour-active
{
	background-color: #feffff !important;
	background-position: -10px 0;
	border: 1px solid #BA1200 !important;
	color: #ba1200 !important;
	cursor:pointer;
	position:relative;
}

span.route-hour-active:after
{
    content: "";
    display: inline-block;
    width: 62px;
    height: 37px;
	/*background-image: url('images/wheels.gif');*/
    background-repeat: no-repeat;
    position: absolute;
    left: 0;
    top: 0;
}

.la-icon-large
{
	font-size: 64px;
}


@media screen and (max-width: 720px)
{
	.libraries 
	{
		text-align: left !important;
		margin: 15px !important;
	}

	.libraries > .library::after
	{
		content: "" !important;
	}
}