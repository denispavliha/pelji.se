#modalConsentShow
{
	color: #BA1200;
}

.modal
{
	z-index: 5001;	
}

#map > p > i
{
	font-size: 36px;
	margin-bottom: 15px;
}

.modal-body > p
{
	font-size: 12px;
}

#credits
{
	z-index: 9000;
	position: absolute;

	bottom: 0px;
	right: 0px;
	padding: 5px 10px;

	border-top-left-radius: 5px;

	background: rgba(0,0,0,0.6);	
}

#credits > a
{
	margin: 0 10px;
	color: #d0d0d0;
	text-decoration: none;
	font-size: 12px; 
}

#credits > a:hover
{ 
	color: #FFFFFF;
	text-decoration: underline;
}

.leaflet-control
{
	display: none !important;
}

.leaflet-popup-content-wrapper, .leaflet-popup-tip
{
	font-size: 14px; 
	font-family: 'Roboto', sans-serif;
	color: #BA1200;
	
	background-color: #feffff;
	background-image: url('images/tooltip-background.png');
	background-position: bottom right;
	background-repeat: no-repeat;

	display: flex;
	align-items: center;
	justify-content: center;
} 

.leaflet-popup-tip
{
	background: #feffff !important;
}

.leaflet-popup-content
{
	padding: 20px 10px;
}
	
.bus-tooltip-number
{
	font-size: 24px;
	line-height: 50px;

	font-family: 'Roboto', sans-serif;
	font-weight: 500;
	text-align: center;
	color: #ffffff;

	width: 50px;
	height: 50px;
	margin-top: -10px;

	border-radius: 50%;
	background-color: #BA1200;
	/*background-image: url('images/buspad.png');*/

	box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.60);
	-webkit-box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.60);
	-moz-box-shadow: 0px 0px 20px 0px rgba(0,0,0,0.60);
}


.btn-tooltip-track
{
	margin-top: 15px;
	width: auto;
}

